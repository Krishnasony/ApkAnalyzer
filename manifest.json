{
  "manifest_version": 3,
  "name": "APK Size Analyzer",
  "version": "1.0.2",
  "description": "Compare APK files and analyze size differences with detailed breakdowns by folder, file extension, and 100+ third-party SDKs.",
  "permissions": [],
  "action": {
    "default_popup": "popup.html",
    "default_title": "APK Size Analyzer",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "homepage_url": "https://github.com/Krishnasony/ApkAnalyzer",
  "author": "Krishna Sony"
}
